{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "dom",
      "esnext"
    ],
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "./dist",
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitThis": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}